<?xml version="1.0" encoding="UTF-8"?>

<root>
  <component name="lu.uni.trux.raicc.setPendingIntentTemplate.TargetActivity" type="Activity"/>
  <component name="lu.uni.trux.raicc.setPendingIntentTemplate.MainActivity" type="Activity">
    <manifest>
      <intent_filter action="android.intent.action.MAIN" category="android.intent.category.LAUNCHER"/>
    </manifest>
    <sender destination="lu.uni.trux.raicc.setPendingIntentTemplate.TargetActivity"/>
  </component>
  <component name="lu.uni.trux.raicc.setPendingIntentTemplate.MyWidgetProvider" type="Receiver">
    <manifest>
      <intent_filter action="android.appwidget.action.APPWIDGET_UPDATE"/>
    </manifest>
    <sender destination="lu.uni.trux.raicc.setPendingIntentTemplate.TargetActivity"/>
  </component>
</root>
