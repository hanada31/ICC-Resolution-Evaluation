<?xml version="1.0" encoding="UTF-8"?>

<root>
  <component name="felixwiemuth.lincal.ui.SettingsActivity" type="Activity">
    <sender destination="felixwiemuth.lincal.ui.SettingsActivity, felixwiemuth.lincal.ui.SettingsFragment"/>
  </component>
  <component name="felixwiemuth.lincal.ui.AddCalendarActivity" type="Activity">
    <manifest>
      <intent_filter action="android.intent.action.VIEW" category="android.intent.category.DEFAULT" scheme="file" host="*"/>
      <intent_filter action="android.intent.action.VIEW" category="android.intent.category.DEFAULT"/>
    </manifest>
    <sender destination="felixwiemuth.lincal.ui.SettingsActivity, interICC_android.intent.action.OPEN_DOCUMENT, android.intent.category.OPENABLE, felixwiemuth.lincal.NotificationService, felixwiemuth.lincal.ui.CalendarListActivity" action="android.intent.action.OPEN_DOCUMENT" category="android.intent.category.OPENABLE" type="*/*"/>
  </component>
  <component name="felixwiemuth.lincal.LinCalBroadcastReceiver" type="Receiver">
    <manifest>
      <intent_filter action="android.intent.action.BOOT_COMPLETED"/>
    </manifest>
    <sender destination="felixwiemuth.lincal.NotificationService"/>
  </component>
  <component name="felixwiemuth.lincal.NotificationService" type="Service">
    <sender destination="felixwiemuth.lincal.NotificationService, felixwiemuth.lincal.ui.CalendarListActivity"/>
  </component>
  <component name="org.acra.dialog.CrashReportDialog" type="Activity">
    <sender destination="org.acra.sender.SenderService"/>
  </component>
  <component name="org.acra.sender.SenderService" type="Service"/>
  <component name="felixwiemuth.lincal.ui.CalendarListActivity" type="Activity">
    <manifest>
      <intent_filter action="android.intent.action.MAIN" category="android.intent.category.LAUNCHER"/>
    </manifest>
    <sender destination="felixwiemuth.lincal.ui.SettingsActivity, felixwiemuth.lincal.ui.AddCalendarActivity, felixwiemuth.lincal.ui.CalendarViewFragment, felixwiemuth.lincal.ui.CalendarViewActivity"/>
  </component>
  <component name="felixwiemuth.lincal.ui.CalendarViewActivity" type="Activity">
    <sender destination="felixwiemuth.lincal.ui.SettingsActivity, felixwiemuth.lincal.ui.CalendarViewFragment"/>
  </component>
</root>
