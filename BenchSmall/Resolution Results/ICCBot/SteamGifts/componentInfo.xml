<?xml version="1.0" encoding="UTF-8"?>

<root>
  <component name="net.mabako.steamgifts.activities.DetailActivity" type="Activity">
    <sender destination="net.mabako.steamgifts.fragments.GiveawayGroupListFragment, net.mabako.steamgifts.activities.DetailActivity, net.mabako.steamgifts.fragments.GiveawayWinnerListFragment, net.mabako.sgtools.SGToolsDetailFragment, net.mabako.steamgifts.activities.LoginActivity, net.mabako.steamgifts.fragments.DiscussionDetailFragment, net.mabako.steamgifts.fragments.HiddenGamesFragment, net.mabako.steamgifts.fragments.WhitelistBlacklistFragment, net.mabako.steamgifts.fragments.UserDetailFragment"/>
  </component>
  <component name="net.mabako.steamgifts.activities.MainActivity" type="Activity">
    <manifest>
      <intent_filter action="android.intent.action.MAIN" category="android.intent.category.LAUNCHER"/>
    </manifest>
    <sender destination="net.mabako.steamgifts.activities.DetailActivity, net.mabako.steamgifts.activities.AboutActivity, net.mabako.steamgifts.activities.LoginActivity, net.mabako.steamgifts.intro.IntroActivity, net.mabako.steamgifts.activities.SettingsActivity"/>
  </component>
  <component name="net.mabako.steamgifts.activities.SettingsActivity" type="Activity">
    <sender destination="net.mabako.steamgifts.activities.DetailActivity, net.mabako.steamgifts.activities.SettingsActivity$SettingsFragment, net.mabako.steamgifts.activities.SyncActivity" action="android.support.customtabs.action.CustomTabsService"/>
  </component>
  <component name="net.mabako.steamgifts.intro.IntroActivity" type="Activity">
    <sender destination="net.mabako.steamgifts.intro.IntroActivity"/>
  </component>
  <component name="net.mabako.steamgifts.receivers.CheckForWonGiveaways" type="Receiver">
    <sender destination="net.mabako.steamgifts.receivers.CheckForNewMessages" action="delete"/>
  </component>
  <component name="net.mabako.steamgifts.activities.UrlHandlingActivity" type="Activity">
    <manifest>
      <intent_filter action="android.intent.action.VIEW" category="android.intent.category.BROWSABLE, android.intent.category.DEFAULT" scheme="http" path="/giveaways/search" host="steamgifts.com"/>
    </manifest>
    <sender destination="net.mabako.steamgifts.activities.DetailActivity, net.mabako.steamgifts.activities.MainActivity, net.mabako.steamgifts.activities.LoginActivity, net.mabako.steamgifts.activities.WebViewActivity, interICC_android.intent.action.VIEW, " action="android.intent.action.VIEW, android.support.customtabs.action.CustomTabsService"/>
  </component>
  <component name="net.mabako.steamgifts.activities.AboutActivity" type="Activity">
    <sender destination="net.mabako.steamgifts.fragments.AboutFragment, net.mabako.steamgifts.activities.DetailActivity, net.mabako.steamgifts.activities.LoginActivity"/>
  </component>
  <component name="net.mabako.steamgifts.activities.LoginActivity" type="Activity"/>
  <component name="net.mabako.steamgifts.activities.WebViewActivity" type="Activity">
    <sender destination="net.mabako.steamgifts.activities.DetailActivity, net.mabako.steamgifts.activities.MainActivity, net.mabako.steamgifts.activities.LoginActivity, net.mabako.steamgifts.activities.WebViewActivity, interICC_android.intent.action.VIEW, " action="android.intent.action.VIEW"/>
  </component>
  <component name="net.mabako.steamgifts.receivers.BootReceiver" type="Receiver">
    <manifest>
      <intent_filter action="android.intent.action.BOOT_COMPLETED, android.intent.action.QUICKBOOT_POWERON"/>
    </manifest>
    <sender destination="java.lang.Class"/>
  </component>
  <component name="net.mabako.sgtools.SGToolsLoginActivity" type="Activity"/>
  <component name="net.mabako.steamgifts.receivers.CheckForNewMessages" type="Receiver">
    <sender destination="net.mabako.steamgifts.receivers.CheckForWonGiveaways, net.mabako.steamgifts.receivers.CheckForNewMessages" action="android.intent.action.VIEW, delete"/>
  </component>
  <component name="com.squareup.picasso.Dispatcher$NetworkBroadcastReceiver" type="Receiver">
    <manifest>
      <intent_filter action="android.net.conn.CONNECTIVITY_CHANGE, android.intent.action.AIRPLANE_MODE"/>
    </manifest>
  </component>
  <component name="net.mabako.steamgifts.activities.WriteCommentActivity" type="Activity">
    <sender destination="net.mabako.steamgifts.fragments.SingleCommentFragment, net.mabako.steamgifts.fragments.WriteCommentFragment"/>
  </component>
  <component name="net.mabako.steamgifts.activities.SyncActivity" type="Activity">
    <sender destination="net.mabako.steamgifts.fragments.SyncFragment, net.mabako.steamgifts.activities.DetailActivity, net.mabako.steamgifts.activities.LoginActivity"/>
  </component>
</root>
