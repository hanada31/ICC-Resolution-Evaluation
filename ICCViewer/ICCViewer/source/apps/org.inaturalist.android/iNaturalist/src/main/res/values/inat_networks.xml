<?xml version="1.0" encoding="utf-8"?>
<resources>
     <!--  The various iNaturalist networks (note that these values are used to load other configurations
    	and strings: For example, inat_country_naturlista, inat_country_naturewatchnz, ... -->
    <string-array name="inat_networks">
        <item>inaturalist</item> <!--  iNaturalist must be first -->
        <item>inaturalistcanada</item>
        <item>naturalistacolombia</item>
        <item>naturalista</item>
        <item>naturewatchnz</item>
        <item>biodiversity4all</item>
        <item>inaturalistpanama</item>
    </string-array>

    <!-- The names of the iNat networks -->
    <string name="network_inaturalist">iNaturalist</string>
    <string name="network_naturalista">NaturaLista</string>
    <string name="network_naturewatchnz">iNaturalist NZ - Mātaki Taiao</string>
    <string name="network_inaturalistcanada">iNaturalist Canada</string>
    <string name="network_naturalistacolombia">Naturalista Colombia</string>
    <string name="network_biodiversity4all">Biodiversity4all</string>
    <string name="network_inaturalistpanama">iNaturalist Panamá</string>

    <!-- If value==1 -> change app name/title in the Android launcher, when network is changed.
    Otherwise, just keep it as the original app name ("iNaturalist") -->
    <string name="change_app_title_inaturalist">1</string>
    <string name="change_app_title_naturalista">0</string>
    <string name="change_app_title_naturewatchnz">0</string>
    <string name="change_app_title_inaturalistcanada">0</string>
    <string name="change_app_title_naturalistacolombia">0</string>
    <string name="change_app_title_biodiversity4all">1</string>
    <string name="change_app_title_inaturalistpanama">0</string>


    <!-- The ISO-3166 alpha-2 country code to use for each network type - used when detecting the user's country -->
    <string name="inat_country_inaturalist"></string> <!-- Empty - any country -->
    <string name="inat_country_naturalista">mx</string> <!-- Mexico -->
    <string name="inat_country_naturewatchnz">nz</string> <!-- New Zealand -->
    <string name="inat_country_inaturalistcanada">ca</string> <!-- Canada -->
    <string name="inat_country_naturalistacolombia">co</string> <!-- Colombia -->
    <string name="inat_country_biodiversity4all">pt</string> <!-- Portugal -->
    <string name="inat_country_inaturalistpanama">pa</string> <!-- Panama -->

     <!-- The country display name to use for each network type - used when detecting the user's country -->
    <string name="inat_country_name_inaturalist">Global</string>
    <string name="inat_country_name_naturalista">Mexico</string>
    <string name="inat_country_name_naturewatchnz">New Zealand</string>
    <string name="inat_country_name_inaturalistcanada">Canada</string>
    <string name="inat_country_name_naturalistacolombia">Colombia</string>
    <string name="inat_country_name_biodiversity4all">Portugal</string>
    <string name="inat_country_name_inaturalistpanama">Panama</string>

    <!-- The domain endpoint to use for each network type - used when adding new observations -->
    <string name="inat_host_inaturalist">https://www.inaturalist.org</string>
    <string name="inat_host_naturalista">http://www.naturalista.mx</string>
    <string name="inat_host_naturewatchnz">http://inaturalist.nz</string>
    <string name="inat_host_inaturalistcanada">http://www.inaturalist.ca</string>
    <string name="inat_host_naturalistacolombia">http://colombia.inaturalist.org</string>
    <string name="inat_host_biodiversity4all">https://www.biodiversity4all.org/</string>
    <string name="inat_host_inaturalistpanama">https://panama.inaturalist.org/</string>

    <!-- The site id parameter to use for each network type - used when adding new observations -->
    <string name="inat_site_id_inaturalist">1</string>
    <string name="inat_site_id_naturalista">2</string>
    <string name="inat_site_id_naturewatchnz">3</string>
    <string name="inat_site_id_inaturalistcanada">5</string>
    <string name="inat_site_id_naturalistacolombia">6</string>
    <string name="inat_site_id_biodiversity4all">8</string>
    <string name="inat_site_id_inaturalistpanama">13</string>

    <!-- The lexicon name to use for each network type - used when searching for taxa (so we'll
    	know which display name to use for the results) -->
    <string name="inat_lexicon_inaturalist">English</string>
    <string name="inat_lexicon_naturalista">Spanish</string>
    <string name="inat_lexicon_naturewatchnz">English</string>
    <string name="inat_lexicon_inaturalistcanada">English</string>
    <string name="inat_lexicon_naturalistacolombia">Spanish</string>
    <string name="inat_lexicon_biodiversity4all">Portuguese</string>
    <string name="inat_lexicon_inaturalistpanama">Spanish</string>

    <!-- The logo drawable name to use for each network type -->
    <string name="inat_logo_inaturalist">logo_text</string>
    <string name="inat_logo_naturalista">logo_naturalista</string>
    <string name="inat_logo_naturewatchnz">logo_naturewatchnz</string>
    <string name="inat_logo_inaturalistcanada">logo_inaturalistcanada</string>
    <string name="inat_logo_naturalistacolombia">logo_naturalista_colombia</string>
    <string name="inat_logo_biodiversity4all">logo_biodiversity4all</string>
    <string name="inat_logo_inaturalistpanama">logo_panama</string>
</resources>
